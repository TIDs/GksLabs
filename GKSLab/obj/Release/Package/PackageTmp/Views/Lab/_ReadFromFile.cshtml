
@using (Html.BeginForm("FileRead", "Lab", FormMethod.Post, htmlAttributes: new { enctype = "multipart/form-data" }))
{
    <div class="form-horizontal">
        <div class="form-group">
            <div class="col-md-4"></div>
            <input type="file" name="file" class="btn-lg btn-default">
            <div class="col-md-2"></div>
        </div>
        <div class="form-group">
            <div class="col-md-2"></div>
            <button type="submit" class="btn-lg btn-primary">Порахувати</button>
            <div class="col-md-2"></div>
        </div>
    </div>
}
